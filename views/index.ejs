<!DOCTYPE html>
<html lang="en">
    <%- include('./partials/head') %> 
    <body>
        <main>
            <% messages.forEach((message) => { %>

                <article>
                    <h3>From: <%= message.user %></h3>
                    <p>Added: <%= message.added %></p>
                    <hr />
                    <p><%= message.text %></p>
                </article>

            <% }) %>
        </main>
    </body>
</html>